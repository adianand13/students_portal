<%include ../partials/assets2/header%>
<%include ../partials/assets2/sidebar%>

<!--start of main content-->
<div class="main-content">
    <%include ../partials/assets2/navbar.ejs%>
    <%include ../partials/assets2/stats_card.ejs%>
    <!--start of page content-->
    <div class="container-fluid mt--7">
        <div class="row">
            <%page.body.forEach(function(faculty){%>
                <div class="col-xl-4 order-xl-2 mt-5 pb-5 mb-5 mb-xl-0">
                <div class="card card-profile shadow">
                    <div class="row justify-content-center">
                        <div class="col-lg-3 order-lg-2">
                            <div class="card-profile-image">
                                <a href="#">
                                    <img src="../assets2/img/theme/team-4-800x800.jpg" class="rounded-circle">
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="card-header text-center border-0 pt-8 pt-md-4 pb-0 pb-md-4">
                        <div class="d-flex justify-content-between">
                            <a href="#" class="btn btn-sm btn-info mr-4">Connect</a>
                            <a href="#" class="btn btn-sm btn-default float-right">Message</a>
                        </div>
                    </div>
                    <div class="card-body pt-0 pt-md-4">
                        <div class="row">
                            <div class="col">
                                <div class="card-profile-stats d-flex justify-content-center mt-md-5">
                                    <div>
                                        <span class="heading">Code</span>
                                        <span class="description">Department</span>
                                    </div>
                                    <div>
                                        <span class="heading">---</span>
                                        <span class="description">Workload</span>
                                    </div>
                                    <div>
                                        <span class="heading">----</span>
                                        <span class="description">Extra</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="text-center">
                            <h3>
                                <%=faculty.Name%><span class="font-weight-light"><br><%=faculty.username%></span>
                            </h3>
                            <hr class="my-4" />
                            <div class="h5 font-weight-300">
                                <i class="ni location_pin mr-2"></i>
                            </div>
                            <div class="h5 mt-4">
                                <i class="ni business_briefcase-24 mr-2"></i>Designation
                            </div>
                            <div>
                                <i class="ni education_hat mr-2"></i>Maharishi Markendeshwar Deemed to be University
                            </div>
                            <a href="#">Show more</a>
                        </div>
                    </div>
                </div>
            </div>
            <%})%>
        </div>
        <%include ../partials/assets2/footer.ejs%>
    </div>
</div>
<!--end of main content-->
<%include ../partials/assets2/ender.ejs%>