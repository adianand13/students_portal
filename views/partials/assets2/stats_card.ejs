    <!-- Stats card -->
    <div class="header bg-gradient-primary pb-8 pt-5 pt-md-8">
        <div class="container-fluid">
            <div class="header-body">
                <!-- Card stats -->
                <div class="row">
                    <%page.stat_card.forEach(function (card) { %>
                        <div class="col-xl-3 col-lg-6" >
                            <!--adds href to all -->
                            <a href="<%=card.url%>">
                                <div class="card card-stats mb-4 mb-xl-0">
                                    <div class="card-body" >
                                        <div class="row" >
                                            <div class="col" >
                                                <h5 class="card-title text-uppercase text-muted mb-0"><%=card.title%></h5>
                                                <span class="h2 font-weight-bold mb-0"><%=card.value1%></span>
                                            </div>
                                            <div class="col-auto" >
                                                <div  class="icon icon-shape <%=card.color1%> text-white rounded-circle shadow">
                                                    <i class="<%=card.icon%>"></i>
                                                </div>
                                            </div>
                                        </div>
                                        <p href="<%=card.url%>" class="mt-3 mb-0 text-muted text-sm">
                                            <span class="<%=card.color2%> mr-2"><i class="fa fa-arrow-up"></i> <%=card.value2%></span>
                                            <span class="text-nowrap"><%=card.description%></span>
                                        </p>
                                    </div>
                                </div>
                            </a>
                        </div>
                    <%})%>
                </div>
            </div>
        </div>
    </div>
    <!--end of stats card-->